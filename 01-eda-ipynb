   "cell_type": "code",
   "execution_count": 4,
   "id": "dfa2aed2",
   "metadata": {
    "_cell_guid": "7d3f90e3-30d8-4693-be35-bc8c37ad4f94",
    "_uuid": "ccb30fdb-e518-4f00-a9a7-92d2839e785d",
